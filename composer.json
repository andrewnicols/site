{
  "name": "moodle/seed",
  "description": "A Composer project template to seed a new Moodle Site",
  "type": "project",
  "require": {
    "moodle/composer-installer": "^1",
    "moodle/moodle-composer-scaffold": "dev-main",
    "moodle/moodle": "^5.1",
    "cweagans/composer-patches": "^2"
  },
  "extra": {
    "install-path": "moodle/"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.mymoodle.dev"
    }
  ],
  "config": {
    "allow-plugins": {
      "moodle/composer-installer": true,
      "cweagans/composer-patches": true,
      "moodle/moodle-composer-scaffold": true
    }
  }
}
