{
  "name": "moodle/site",
  "description": "Project Template to create a new Moodle Site",
  "type": "project",
  "require": {
    "composer/installers": "dev-moodlePathFromJson",
    "moodle/core-composer-scaffold": "dev-main",
    "moodle/moodle": "dev-composed",
    "fmcorz/moodle-block_xp": "^19.0",
    "abgreeve/moodle-block_stash": "dev-composed",
    "mdjnelson/moodle-mod_customcert": "dev-composed"
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/installers"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/core-composer-scaffold"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/composedmoodle"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/moodle-availability_stash"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/moodle-block_stash"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/moodle-filter_stash"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrewnicols/moodle-mod_customcert"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "moodle/core-composer-scaffold": true
    }
  }
}
